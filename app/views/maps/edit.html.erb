<div class="container">
  <a href="/">トップ</a><i class="fas fa-caret-right f20 ml-1 mr-1"></i>名称変更</a>
</div>

<div class="container">
    <%= form_for @map do |f| %>
        <div class="form-group bold font-japanese">
            <%= f.label :address ,"名称・住所" %>
            <span id="add"><%= f.text_field :address,value: @map.address ,class: "form-control" %></span>
        </div>            
        <div class="actions center">
            <span><%= f.submit "名称変更",class: "btn btn-primary2 bold" %></span>
        </div>
    <% end %>
</div> 
